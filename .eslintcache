 setIsLogin] = useState(true)\n    const toggleIsLogin = () => {\n    setIsLogin(!isLogin)\n    }\n\n    return (\n      <div>\n        <Navbar color=\"light\" light expand=\"md\">\n          <NavbarBrand href=\"/\" style={{color:'light', alignItems:'center'}}>\n            <img src='https://freepngimg.com/thumb/logo/62837-instagram-icons-photography-computer-logo-icon.png' style={{height:'40px'}}/>\n            Nextagram\n          </NavbarBrand>\n          <Collapse isOpen={isOpen} navbar>\n            <Nav className=\"mr-auto\" navbar></Nav>\n          </Collapse>\n              <NavLink style={{cursor:'pointer'}} onClick= {() => {toggle() \n                setIsLogin(true)\n              }}>Log In</NavLink>\n              <NavLink style={{cursor:'pointer'}} onClick= {() => {toggle() \n                setIsLogin(false)}}>\n                Sign Up</NavLink>\n        </Navbar>\n        {\n            isLogin\n                ? <AuthModal \n                    isOpen={isOpen} \n                    toggle={toggle}\n                    modalTitle={\"Login\"}\n                    primaryBtnText={\"Log In\"}\n                    primaryBtnAction={() => {}}\n                    isLogin={isLogin}\n                    toggleIsLogin={toggleIsLogin}\n                  />\n                : <AuthModal \n                    isOpen={isOpen} \n                    toggle={toggle}\n                    modalTitle={\"Sign Up\"}\n                    primaryBtnText={\"Sign Up\"}\n                    primaryBtnAction={() => {}}\n                    isLogin={isLogin}\n                    toggleIsLogin={toggleIsLogin}\n                  />    \n              }\n      </div>\n    );\n  \n}\n\nexport default NavbarDisplay;","/Users/fabianlim/Desktop/NextAcademy/Fullstack November class/Day 13 - Instagram clone/nextagram-2/src/Containers/UserImages.js",["64"],"import axios from 'axios'\nimport {useState, useEffect} from 'react'\n\n\nfunction UserImages(props){\n    const {userId} = props\n\